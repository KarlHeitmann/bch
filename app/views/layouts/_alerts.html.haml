- flash.each do |key, value|
  %div{:class => "alert alert-#{key} alert-dismissible alert-fixed", role: "alert"}
    %button{type:"button", class:"close", data: {dismiss:"alert"}, aria: {label:"Close"}}
      %span{aria:{hidden:"true"}}
        = "&times;".html_safe
    %ul
      %li
        = value
